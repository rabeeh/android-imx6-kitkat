-	u8 *ie;
-
-	if (nw_type & ADHOC_NETWORK) {
-		cap_mask = WLAN_CAPABILITY_IBSS;
-		cap_val = WLAN_CAPABILITY_IBSS;
-	} else {
-		cap_mask = WLAN_CAPABILITY_ESS;
-		cap_val = WLAN_CAPABILITY_ESS;
-	}
-
-	bss = cfg80211_get_bss(ar->wiphy, chan, bssid,

-	if (type == NL80211_IFTYPE_P2P_CLIENT ||
-	    type == NL80211_IFTYPE_P2P_GO) {
-		for (i = ar->max_norm_iface; i < ar->vif_max; i++) {
-			if ((ar->avail_idx_map >> i) & BIT(0)) {
-				*if_idx = i;

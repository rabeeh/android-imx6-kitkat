+static void hexstr_bytearray(char *hexStr, u8 *byteArray, u8 numBytes)
+{
+	u8 i;
+
+	for (i = 0; i < numBytes; i++)
+		byteArray[i] = 16 * _from_hex(hexStr[2 * i + 0]) +
+			       _from_hex(hexStr[2 * i + 1]);
+}

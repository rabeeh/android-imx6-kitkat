-	ath6kl_dbg(ATH6KL_DBG_SDIO, "irq\n");
-
-	ar_sdio = sdio_get_drvdata(func);
-	atomic_set(&ar_sdio->irq_handling, 1);
-	/*
-	 * Release the host during interrups so we can pick it back up when

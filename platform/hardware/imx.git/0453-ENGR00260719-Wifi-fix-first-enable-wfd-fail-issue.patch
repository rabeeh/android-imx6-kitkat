-static bool ath6kl_is_wps_ie(const u8 *pos)
-{
-	return (pos[0] == WLAN_EID_VENDOR_SPECIFIC &&
-		pos[1] >= 4 &&
-		pos[2] == 0x00 && pos[3] == 0x50 && pos[4] == 0xf2 &&
-		pos[5] == 0x04);

-static int ath6kl_set_ap_probe_resp_ies(struct ath6kl_vif *vif,
-					const u8 *ies, size_t ies_len)
-{
-	struct ath6kl *ar = vif->ar;
-	const u8 *pos;
-	u8 *buf = NULL;
-	size_t len = 0;

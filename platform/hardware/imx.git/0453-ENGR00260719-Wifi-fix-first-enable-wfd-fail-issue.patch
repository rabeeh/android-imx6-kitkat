-	if (!virt_scat) {
-		ret = ath6kl_sdio_alloc_prep_scat_req(ar_sdio,
-				MAX_SCATTER_ENTRIES_PER_REQ,
-				MAX_SCATTER_REQUESTS, virt_scat);

-	ath6kl_dbg(ATH6KL_DBG_WLAN_CFG,
-		   "cfg reg_notify %c%c%s%s initiator %d\n",
-		   request->alpha2[0], request->alpha2[1],
-		   request->intersect ? " intersect" : "",
-		   request->processed ? " processed" : "",

-void read_mem(void * addr, uint32_t count, uint32_t size)
+void read_mem(void *addr, uint32_t count, uint32_t size)
 {
 	int i;
-	uint8_t * addr8 = addr;
-	uint16_t * addr16 = addr;
-	uint32_t * addr32 = addr;
-
-	switch (size)
-	{
-		case 1:
-			for (i = 0; i < count; i++) {
-				if ( (i % 16) == 0 )
-					printf("\n0x%08X: ", g_paddr);
-				printf(" %02X", addr8[i]);

 {
 	int i;
-	uint8_t * addr8 = addr;
-	uint16_t * addr16 = addr;
-	uint32_t * addr32 = addr;
-
-	switch (size)
-	{
-		case 1:
-			*addr8 = value;
-			break;
-		case 2:
-			*addr16 = value;
-			break;
-		case 4:
-			*addr32 = value;
-			break;
+	uint8_t *addr8 = addr;
+	uint16_t *addr16 = addr;
+	uint32_t *addr32 = addr;
+
+	switch (size) {
+	case 1:
+		*addr8 = value;
+		break;
+	case 2:
+		*addr16 = value;
+		break;
+	case 4:
+		*addr32 = value;
+		break;
 	}
 }
 

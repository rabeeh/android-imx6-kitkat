@@ -15,7 +15,7 @@ EXES = \
 	$(OBJDIR)/mxc_mpeg4enc_test.out \
 	$(OBJDIR)/mxc_mpeg4dec_test.out
 else
-EXES = 
+EXES =
 endif
 
 all : $(EXES) clean_build
@@ -30,10 +30,6 @@ $(OBJDIR)/%.out : ./%.c
 
 $(OBJDIR)/%.out : $(SRCDIR)/%.c
 
-%.out : %.o 
-	$(VERBOSE)echo "     Building $@"
-	$(VERBOSE)$(CC) $(CFLAGS) -o $@ $< $(LIBS)
-
 include $(EXES:.out=.d)
 
 .PHONY: clean_build

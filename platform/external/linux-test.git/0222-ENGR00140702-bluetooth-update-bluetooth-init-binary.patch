-	do {
-		line = fgets(buffer, LINE_SIZE_MAX, stream);
-
-		if (!line)
-			break;
-
-		skip_space(line);
-
-		if ((line[0] == '/') && (line[1] == '/'))
-			continue;
-
-		if ((line[0] == '#')) {
-
-			if (read_status.section != 0) {
-				perror("error\n");
-
-				if (buffer != NULL)

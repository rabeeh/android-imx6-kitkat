+fi
+print_status
+exit $STATUS
diff --git a/test/mxc_asrc_test/mxc_asrc_test.c b/test/mxc_asrc_test/mxc_asrc_test.c
new file mode 100644
index 0000000..d55ee2b
--- /dev/null
+++ b/test/mxc_asrc_test/mxc_asrc_test.c
@@ -0,0 +1,416 @@
+/*

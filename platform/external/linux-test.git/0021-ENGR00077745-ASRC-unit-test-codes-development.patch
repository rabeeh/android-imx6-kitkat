 4 files changed, 498 insertions(+)
 create mode 100644 test/mxc_asrc_test/Makefile
 create mode 100755 test/mxc_asrc_test/audio8k16S.wav
 create mode 100755 test/mxc_asrc_test/autorun-asrc.sh
 create mode 100644 test/mxc_asrc_test/mxc_asrc_test.c

diff --git a/test/mxc_asrc_test/Makefile b/test/mxc_asrc_test/Makefile
new file mode 100644
index 0000000..a815078
--- /dev/null
+++ b/test/mxc_asrc_test/Makefile
@@ -0,0 +1,25 @@
+#
+# Makefile for OSS sound driver test app.
+#
+# list of platforms which did not want this test case
+EXCLUDE_LIST:= IMX21ADS IMX27ADS IMX31ADS IMX32ADS IMX37_3STACK
+
+ifeq (,$(findstring $(PLATFORM), $(EXCLUDE_LIST)))
+OBJS = \
+	$(OBJDIR)/mxc_asrc_test.out \
+	$(OBJDIR)/autorun-asrc.sh
+else
+OBJS =
+endif
+
+all : $(OBJS)
+	cp audio8k16S.wav $(OBJDIR)
+
+.PHONY: clean
+clean :
+	rm -f $(OBJS)
+
+#
+# include the Rules
+#
+include ../make.rules

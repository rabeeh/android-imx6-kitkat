 	lib_wb_amr_dec_arm9_elinux			\
-	lib_omx_aac_enc_v2_arm11_elinux
+	lib_omx_aac_enc_v2_arm11_elinux \
+	lib_amr_parser_arm11_elinux.3.0 \
 
 # Omx excluded libs

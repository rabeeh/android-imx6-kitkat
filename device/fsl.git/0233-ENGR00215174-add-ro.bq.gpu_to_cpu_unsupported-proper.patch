     setprop hwc.enable_dither   1
diff --git a/sabresd_6dq/init.rc b/sabresd_6dq/init.rc
index de323c6..3bf3d95 100755
--- a/sabresd_6dq/init.rc
+++ b/sabresd_6dq/init.rc
@@ -37,6 +37,7 @@ on boot
     setprop debug.sf.showfps    0

index df19bf5..ff8185c 100755
--- a/common/tools/fsl-sdcard-partition.sh
+++ b/common/tools/fsl-sdcard-partition.sh
@@ -4,7 +4,7 @@
 BOOTLOAD_RESERVE=8
 BOOT_ROM_SIZE=8
